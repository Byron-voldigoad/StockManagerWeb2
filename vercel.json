{
  "version": 2,
  "buildCommand": "ng build --configuration production",
  "outputDirectory": "dist/stockmanager_web",
  "routes": [
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|json|txt|woff|woff2|ttf|eot|svg))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}